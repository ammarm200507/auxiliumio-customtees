{
  "rewrites": [
    {
      "source": "/mvp",
      "destination": "/mvp.html"
    },
    {
      "source": "/mvp/",
      "destination": "/mvp.html"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}

